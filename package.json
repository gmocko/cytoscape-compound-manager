  },
  "devDependencies": {
    "jest": "^29.7.0"
  },
  "peerDependencies": {
    "cytoscape": "^3.0.0",
    "cytoscape-cola": "^2.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {},
    "moduleFileExtensions": ["js", "mjs"],
    "testMatch": ["**/__tests__/**/*.test.js"]
